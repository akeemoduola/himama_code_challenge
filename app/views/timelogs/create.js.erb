if (<%= @clock_in_button_disabled %>) {
  document.getElementById('clock_in_button').disabled = true
} else {
  document.getElementById('clock_in_button').disabled = false
}

if (<%= @clock_out_button_disabled %>) {
  document.getElementById('clock_out_button').disabled = true
} else {
  document.getElementById('clock_out_button').disabled = false
}